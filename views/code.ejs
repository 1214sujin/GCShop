<table class="table table-bordered border-primary">
    <thead>
        <th>대분류</th><th>대분류명</th><th>소분류</th><th>소분류명</th><th>시작일</th><th>종료일</th>
        <% if (isU) { %><th colspan="2">메뉴</th><% } %>
    </thead> 
   
    <% for (let i=0; i < code.length; i++) { %>
        <tr><td><%= code[i].main_id %></td>
            <td><%= code[i].main_name %></td>
            <td><%= code[i].sub_id %></td>
            <td><%= code[i].sub_name %></td>
            <td><%= code[i].start %></td>
            <td><%= code[i].end %></td>
            <% if (isU) { %>
                <td><a href="/code/update/<%= code[i].main_id %>/<%= code[i].sub_id %>">수정</a></td>
                <td><a href="/code/delete/<%= code[i].main_id %>/<%= code[i].sub_id %>" onclick="return confirm('정말로 삭제하시겠습니까?')">삭제</a></td>
            <% } %>
        </tr>
    <% } %>

</table>